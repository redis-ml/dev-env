{
    "name": "awesome-babylon-starter",
    "version": "1.0.0",
    "description": "Starter template for Babylon.js projects using Webpack, TypeScript, Babel, Sass and PUG.",
    "scripts": {
        "build": "webpack --config webpack.config.js --progress --profile --colors",
        "start": "webpack serve --config webpack.config.dev.js --inline --hot --open",
        "type-check": "tsc --noEmit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": {
        "name": "Brian Hay ",
        "email": "brian@virtualrealms.com.au"
    },
    "main": "dist/bundle.js",
    "types": "dist/types/index.d.ts",
    "license": "MIT",
    "devDependencies": {
        "@bazel/buildifier": "^4.2.5",
        "@bazel/ibazel": "^0.15.10",
        "@bazel/typescript": "^5.0.2",
        "@types/react": "^16.9.5",
        "@types/react-dom": "^16.9.1",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^3.2.0",
        "csstype": "^2.2.0",
        "http-server": "^0.11.1",
        "source-map": "^0.7.3",
        "@types/source-map": "0.5.2",
        "fork-ts-checker-webpack-plugin": "^6.1.0",
        "html-loader": "^2.0.0",
        "html-webpack-plugin": "^5.1.0",
        "sass": "^1.49.7",
        "sass-loader": "^12.4.0",
        "string-replace-loader": "^3.0.1",
        "style-loader": "^2.0.0",
        "ts-loader": "^8.0.17",
        "typescript": "^3.6.3",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^5.22.0",
        "webpack-cli": "^4.5.0",
